    "init_dev": "yarn --dev && yarn dev"
  },
  "dependencies": {
    "@mdx-js/loader": "latest",
    "@mdx-js/react": "latest",
    "@next/mdx": "latest",
    "mdx.macro": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "prettier": "latest",
    "puppeteer": "latest"
  }
}
