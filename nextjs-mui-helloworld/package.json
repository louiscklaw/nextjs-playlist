{
  "name": "nextjs-mui-helloworld",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clear": "rm -rf node_modules && rm -rf .next",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "prettier_file": "prettier --write \"./pages/**/*.{js,jsx}\"",
    "gitUpdate": "git add . && git commit -m\"update nextjs-mui-helloworld,\"",
    "init_dev": "yarn --dev && yarn dev",
    "docker_rebuild": "docker compose up -d --build",
"docker_rebuild_logs": "docker compose up -d --build && docker compose logs -f",
    "into_docker": "docker compose exec -it next /bin/bash"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "next": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "prettier": "latest",
    "puppeteer": "latest"
  }
}